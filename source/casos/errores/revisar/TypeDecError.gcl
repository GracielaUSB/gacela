program main |[

  proc sumar : (inout m : int, in n : int) 
  [
      {pre m >= 0 /\ n >= 0 pre}  
      |[
          m := m + n
      ]|  
      {post m <= m + n post}
  ]

  func fibonacci : (m : int, n : int) -> int
  |[
      m + n
  ]|

  |[
      var   foo, bar := true, false : int;
      sumar(foo, bar);
      bar := fibonacci(bar, foo)
  ]|
]|
